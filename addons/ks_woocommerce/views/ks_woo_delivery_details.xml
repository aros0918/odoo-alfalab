<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <!--Window Action for WooCommerce Deliveries-->
    <record model="ir.actions.act_window" id="action_woocommerce_deliveries">
        <field name="name">WooCommerce Deliveries</field>
        <field name="res_model">stock.picking</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[('sale_id.ks_woo_order_id','&gt;',0)]</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                **All the WooCommerce Deliveries will appear here.
            </p>
        </field>
    </record>

</odoo>